{
  "name": "electron-update-test",
  "productName": "electron-update-test",
  "version": "1.0.3",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "repository": "github:mail4andrey/electron-update-test",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "generate-barrels": "barrelsby -c barrelsby.json --singleQuotes",
    "sort-package.json": "sort-package-json"
  },
  "keywords": [],
  "author": {
    "name": "mail4andrey",
    "email": "mail4andrey@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron-update-test"
          },
          "platforms": [
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {},
          "platforms": [
            "linux"
          ]
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {},
          "platforms": [
            "linux"
          ]
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.52",
    "@electron-forge/maker-rpm": "6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "6.0.0-beta.52",
    "@electron-forge/maker-zip": "6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
    "@types/react": "16.9.44",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@typescript-eslint/eslint-plugin": "3.8.0",
    "@typescript-eslint/parser": "3.8.0",
    "awesome-typescript-loader": "^5.2.1",
    "barrelsby": "2.2.0",
    "css-loader": "4.2.1",
    "electron": "9.2.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsdoc": "30.2.1",
    "eslint-plugin-optimize-regex": "1.2.0",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-unicorn": "21.0.0",
    "fork-ts-checker-webpack-plugin": "5.0.14",
    "node-loader": "1.0.1",
    "style-loader": "1.2.1",
    "ts-loader": "8.0.2",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "electron-log": "4.2.2",
    "electron-squirrel-startup": "1.0.0",
    "fast-xml-parser": "^3.17.4",
    "mobx": "5.15.5",
    "mobx-react": "6.2.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-oc": "1.0.0-alpha.6",
    "react-router-dom": "5.2.0",
    "update-electron-app": "1.5.0"
  }
}

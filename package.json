{
  "name": "selfiebox-spinner",
  "productName": "Spinner (SelfieBox)",
  "version": "1.0.0",
  "description": "Spinner (SelfieBox)",
  "main": "./.webpack/main",
  "scripts": {
    "package-kiosk": "@powershell Copy-Item package.json -Destination package.copy.json && package-json-merge package.copy.json src/applications/kiosk/package.json > package.json && @powershell Remove-Item package.copy.json",
    "package-clipmaker": "@powershell Copy-Item package.json -Destination package.copy.json && package-json-merge package.copy.json src/applications/clipmaker/package.json > package.json && @powershell Remove-Item package.copy.json",
    "package-spinner": "@powershell Copy-Item package.json -Destination package.copy.json && package-json-merge package.copy.json src/applications/spinner/package.json > package.json && @powershell Remove-Item package.copy.json",
    "start": "set DEFAULTPORT=8003&& set APPLICATION=spinner&& rimraf dist-front && webpack --config webpack.front.common.js --env build_mode=development && electron-forge start",
    "package": "set DEFAULTPORT=8003&& set APPLICATION=spinner&& rimraf dist-front && webpack --config webpack.front.common.js && electron-forge package",
    "make": "set DEFAULTPORT=8003&& set APPLICATION=spinner&& rimraf dist-front && webpack --config webpack.front.common.js && electron-forge make",
    "publish": "set DEFAULTPORT=8003&& set APPLICATION=spinner&& rimraf dist-front && webpack --config webpack.front.common.js && electron-forge publish",
    "lint": "eslint --ext .ts .",
    "generate-barrels": "barrelsby -c barrelsby.json --singleQuotes",
    "sort-package.json": "sort-package-json"
  },
  "keywords": [],
  "author": {
    "name": "SelfieBox96",
    "email": "selfiebox96@mail.ru"
  },
